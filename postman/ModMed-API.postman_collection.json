{
  "info": {
    "name": "ModMed API",
    "description": "ModMed EMA API integration endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "modmedBaseUrl",
      "value": "https://stage.ema-api.com/ema-dev",
      "type": "string"
    },
    {
      "key": "firmPrefix",
      "value": "entpmsandbox393",
      "type": "string"
    },
    {
      "key": "apiKey",
      "value": "f69902ad-c2bc-4b30-aa89-e136d26a04b3",
      "type": "string"
    },
    {
      "key": "username",
      "value": "fhir_pmOYS",
      "type": "string"
    },
    {
      "key": "password",
      "value": "NmrxdT7I34",
      "type": "string"
    },
    {
      "key": "accessToken",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Get Access Token",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "X-API-Key", "value": "{{apiKey}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"{{username}}\",\n  \"password\": \"{{password}}\",\n  \"firm_prefix\": \"{{firmPrefix}}\"\n}"
            },
            "url": {
              "raw": "{{modmedBaseUrl}}/auth/login",
              "host": ["{{modmedBaseUrl}}"],
              "path": ["auth", "login"]
            }
          }
        }
      ]
    },
    {
      "name": "FHIR Patients",
      "item": [
        {
          "name": "Get All Patients",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{accessToken}}"},
              {"key": "X-API-Key", "value": "{{apiKey}}"}
            ],
            "url": {
              "raw": "{{modmedBaseUrl}}/fhir/Patient",
              "host": ["{{modmedBaseUrl}}"],
              "path": ["fhir", "Patient"]
            }
          }
        },
        {
          "name": "Search Patients by Name",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{accessToken}}"},
              {"key": "X-API-Key", "value": "{{apiKey}}"}
            ],
            "url": {
              "raw": "{{modmedBaseUrl}}/fhir/Patient?name=John",
              "host": ["{{modmedBaseUrl}}"],
              "path": ["fhir", "Patient"],
              "query": [
                {"key": "name", "value": "John"}
              ]
            }
          }
        },
        {
          "name": "Get Patient by ID",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{accessToken}}"},
              {"key": "X-API-Key", "value": "{{apiKey}}"}
            ],
            "url": {
              "raw": "{{modmedBaseUrl}}/fhir/Patient/123",
              "host": ["{{modmedBaseUrl}}"],
              "path": ["fhir", "Patient", "123"]
            }
          }
        },
        {
          "name": "Create Patient",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Authorization", "value": "Bearer {{accessToken}}"},
              {"key": "X-API-Key", "value": "{{apiKey}}"},
              {"key": "Content-Type", "value": "application/fhir+json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"resourceType\": \"Patient\",\n  \"name\": [{\n    \"use\": \"official\",\n    \"family\": \"Doe\",\n    \"given\": [\"Jane\"]\n  }],\n  \"gender\": \"female\",\n  \"birthDate\": \"1990-05-15\",\n  \"telecom\": [{\n    \"system\": \"phone\",\n    \"value\": \"555-123-4567\"\n  }, {\n    \"system\": \"email\",\n    \"value\": \"jane.doe@email.com\"\n  }]\n}"
            },
            "url": {
              "raw": "{{modmedBaseUrl}}/fhir/Patient",
              "host": ["{{modmedBaseUrl}}"],
              "path": ["fhir", "Patient"]
            }
          }
        }
      ]
    },
